{
	"short_name": "AVADoma",
	"name": "AVADoma - Domains for IPFS, AVAX Wallet",
	"description": "IPFS domain .ava, .web3, .dapp, .ipfs, .defi, .gamefi",
	"version": "0.1",
	"manifest_version": 3,
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"action": {
		"default_icon": {
			"16": "16.png",
			"24": "24.png",
			"32": "32.png",
			"48": "48.png",
			"128": "128.png"
		},
		"default_title": "AVADoma",
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs", "storage", "webRequest", "scripting"],
	"host_permissions": ["<all_urls>"]
}
